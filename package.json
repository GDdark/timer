{
  "name": "clock-timer.js",
  "version": "1.1.2",
  "description": "Timing Events tied to clock.js",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "dist": "browserify ./src/index.js -t babelify -s ClockTimer -o dist/clock-timer.js && uglifyjs dist/clock-timer.js > dist/clock-timer.min.js",
    "prepublish": "rimraf ./dist && tsc -d",
    "test": "npm run prepublish && mocha --compilers js:babel-register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gamestdio/clock-timer.js.git"
  },
  "keywords": [
    "clock",
    "timer",
    "timer-events",
    "interval",
    "timeout",
    "deltatime"
  ],
  "author": "Endel Dreyer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gamestdio/clock-timer.js/issues"
  },
  "homepage": "https://github.com/gamestdio/clock-timer.js#readme",
  "dependencies": {
    "clock.js": "^1.1.0"
  },
  "devDependencies": {
    "assert": "^1.3.0",
    "babel-cli": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "mocha": "^2.3.4",
    "rimraf": "^2.5.2",
    "typescript": "^1.8.10",
    "uglify-js": "^2.6.1"
  }
}
